<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<sectionGroup name="elmah">
			<section name="security" requirePermission="false" type="Elmah.SecuritySectionHandler, Elmah" />
			<section name="errorLog" requirePermission="false" type="Elmah.ErrorLogSectionHandler, Elmah" />
		</sectionGroup>
	</configSections>
	<elmah>
		<security allowRemoteAccess="0" />
		<errorLog type="Elmah.SqlErrorLog, Elmah" connectionStringName="elmah-sqlserver" />
	</elmah>
	<system.web>
		<compilation debug="true" targetFramework="4.8" />
		<httpRuntime targetFramework="4.8" />
	</system.web>
	<system.webServer>
		<validation validateIntegratedModeConfiguration="false" />
		<modules>
			<add name="ErrorLog" type="Elmah.ErrorLogModule, Elmah" preCondition="managedHandler" />
			<add name="Hooks" type="ParadimeWeb.WorkflowGen.WebServer.Modules.Hooks, ParadimeWeb.WorkflowGen.Web.UI.WebForms" preCondition="managedHandler" />
			<add name="Cookies" type="ParadimeWeb.WorkflowGen.WebServer.Modules.Cookies, ParadimeWeb.WorkflowGen.Web.UI.WebForms" preCondition="managedHandler" />
		</modules>
		<handlers>
			<add name="Elmah" path="elmah.axd" verb="POST,GET,HEAD" type="Elmah.ErrorLogPageFactory, Elmah" preCondition="integratedMode" />
		</handlers>
	</system.webServer>
</configuration>